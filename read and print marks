#include <stdio.h>
int main() {
    int m1, m2, m3, m4, m5, m6;
    printf("Enter marks of six subjects:\n");
    scanf("%d %d %d %d %d %d", &m1, &m2, &m3, &m4, &m5, &m6);
    printf("\nMarks entered are:\n");
    printf("Subject 1: %d\n", m1);
    printf("Subject 2: %d\n", m2);
    printf("Subject 3: %d\n", m3);
    printf("Subject 4: %d\n", m4);
    printf("Subject 5: %d\n", m5);
    printf("Subject 6: %d\n", m6);
    return 0;
}
