#include <stdio.h>
int isArmstrong(int n)
{
    int sum=0, temp=n, digit;
    while(temp>0){
        digit=temp%10;
        sum+=digit*digit*digit;
        temp/=10;
    }
    return sum==n;
}
int main(){
    int n;
    scanf("%d",&n);
    if(isArmstrong(n))
        printf("Armstrong number");
    else
        printf("Not an Armstrong number");
    return 0;
}
