#include <stdio.h>
void bubbleSort(int *arr,int n)
{
    int temp;
    for(int i=0;i<n-1;i++){
        for(int j=0;j<n-1-i;j++){
            if(*(arr+j)>*(arr+j+1)){
                temp=*(arr+j);
                *(arr+j)=*(arr+j+1);
                *(arr+j+1)=temp;
            }
        }
    }
}
int binarySearch(int *arr,int n,int key)
{
    int low=0,high=n-1,mid;
    while(low<=high){
        mid=(low+high)/2;
        if(*(arr+mid)==key)
            return mid;
        else if(*(arr+mid)<key)
            low=mid+1;
        else
            high=mid-1;
    }
    return -1;
}
int main()
{
    int n,key;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++)
        scanf("%d",&arr[i]);
    scanf("%d",&key);
    bubbleSort(arr,n);
    int pos=binarySearch(arr,n,key);
    if(pos!=-1)
        printf("Element found at position %d",pos+1);
    else
        printf("Element not found");
    return 0;
}
