#include <stdio.h>
int main(){
    int n,temp;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++)
        scanf("%d",&arr[i]);
    int *p=arr;
    for(int i=0;i<n-1;i++){
        for(int j=0;j<n-1-i;j++){
            if(*(p+j)>*(p+j+1)){
                temp=*(p+j);
                *(p+j)=*(p+j+1);
                *(p+j+1)=temp;
            }
        }
    }
    for(int i=0;i<n;i++)
        printf("%d ",*(p+i));
    return 0;
}
