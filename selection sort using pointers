#include <stdio.h>
int main(){
    int n,temp;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++)
        scanf("%d",&arr[i]);
    int *p=arr;
    for(int i=0;i<n-1;i++){
        int min=i;
        for(int j=i+1;j<n;j++){
            if(*(p+j)<*(p+min))
                min=j;
        }
        if(min!=i){
            temp=*(p+i);
            *(p+i)=*(p+min);
            *(p+min)=temp;
        }
    }
    for(int i=0;i<n;i++)
        printf("%d ",*(p+i));
    return 0;
}
