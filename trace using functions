#include <stdio.h>
int trace_mat(int n, int mat[10][10]){
    int trace=0;
    for(int i=0;i<n;i++)
        trace+=mat[i][i];
    return trace;
}
int main(){
    int n;
    scanf("%d",&n);
    int mat[10][10];
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++)
            scanf("%d",&mat[i][j]);
    }
    printf("Trace of matrix: %d",trace_mat(n,mat));
    return 0;
}
